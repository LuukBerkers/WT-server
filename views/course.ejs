<html>
  <head>
		<title><%= title %> - Course Page</title>
		<link rel="stylesheet" href="stylesheets/style.css">
  </head>
  <body>
    <header>
      <h1><%= title %></h1>
    </header>
    <nav>
      <ul>
        <li><a href="/">Home</a></li>
        <li><a href="/courses/all">Courses</a></li>
        <li><a href="/login">Login</a></li>
        <li><a href="/user">Profile</a></li>
      </ul>
    </nav>
    <t2>Coursecode: <%= code %></t2>
	  <table>
 	   <tr>
   		  <th>Code</th>
        <td><%= code %></td>
      </tr>
      <tr>
        <th>Program</th>
        <td><%= program %></td>
      </tr>
      <tr>
        <th>Level</th>
        <td><%= level %></td>
      </tr>
      <tr>
        <th>Semester</th>
        <td><%= semester %></td>
      </tr>
      <tr>
        <th>Timeslot</th>
        <td><%= timeslot %></td>
      </tr>
		</table>
	<p>Course description: <%= description %></p>
	<p>Teacher: <%= teacher %></p>
	<img src=<%= photo %>>
  <form action="/courses/<%= code %>/register" method="POST">
    <input type="submit" value="Register">
  </form>
  <form action="/courses/<%= code %>/unregister" method="POST">
    <input type="submit" value="Unregister">
  </form>
  </body>
</html>